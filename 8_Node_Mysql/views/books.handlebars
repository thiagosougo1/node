<h1>Livros disponíveis:</h1>
<div class="books-container">
  <a href="/">Adicionar livro</a>
  {{#each books}}
    <div>
      #{{this.id}}
      -
      <a href="/books/{{this.id}}">
        <span class="bold">{{this.title}} - {{this.pageqtd}} páginas</span>
      </a>

    </div>
  {{/each}}
</div>